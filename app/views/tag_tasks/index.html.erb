<p id="notice"><%= notice %></p>

<h1>Tag Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Task</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_tasks.each do |tag_task| %>
      <tr>
        <td><%= tag_task.tag_id %></td>
        <td><%= tag_task.task_id %></td>
        <td><%= link_to 'Show', tag_task %></td>
        <td><%= link_to 'Edit', edit_tag_task_path(tag_task) %></td>
        <td><%= link_to 'Destroy', tag_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag Task', new_tag_task_path %>
